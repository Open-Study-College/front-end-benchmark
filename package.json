    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.2.3",
    "sass": "^1.49.9"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.2.3",
    "@remix-run/eslint-config": "^1.2.3",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "eslint": "^8.9.0",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">=14"
  }
}
